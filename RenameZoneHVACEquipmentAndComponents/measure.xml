  <class_name>RenameZoneHVACEquipmentAndComponents</class_name>
  <display_name>Rename Zone HVAC Equipment And Components</display_name>
  <description>Renames zone HVAC equipment and zone HVAC equipment components (fans, cooling coils, and heating coils) based on zone name.</description>
  <modeler_description>Loops through zones, gets zone equipment, and checks if each equipment type is initialized. If so, checks if zone equipment components are initialized and gets these components if true. Zone equipment is renamed followed by zone equipment components.</modeler_description>
  <arguments>
    <argument>
      <name>eqpt_type</name>
      <display_name>Zone HVAC Equipment Type</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <choices>
        <choice>
          <value>ZoneHVACFourPipeFanCoil</value>
          <display_name>ZoneHVACFourPipeFanCoil</display_name>
        </choice>
        <choice>
          <value>ZoneHVACPackagedTerminalAirConditioner</value>
          <display_name>ZoneHVACPackagedTerminalAirConditioner</display_name>
        </choice>
        <choice>
          <value>ZoneHVACPackagedTerminalHeatPump</value>
          <display_name>ZoneHVACPackagedTerminalHeatPump</display_name>
        </choice>
        <choice>
          <value>ZoneHVACUnitHeater</value>
          <display_name>ZoneHVACUnitHeater</display_name>
        </choice>
        <choice>
          <value>ZoneHVACWaterToAirHeatPump</value>
          <display_name>ZoneHVACWaterToAirHeatPump</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>rename_hvac_eqpt</name>
      <display_name>Rename zone HVAC equipment?</display_name>
      <type>Boolean</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>rename_hvac_comp</name>
      <display_name>Rename zone HVAC equipment components?</display_name>
      <type>Boolean</type>
      <required>false</required>
      <model_dependent>false</model_dependent>
      <default_value>true</default_value>
      <choices>
        <choice>
          <value>true</value>
          <display_name>true</display_name>
        </choice>
        <choice>
          <value>false</value>
          <display_name>false</display_name>
        </choice>
      </choices>
    </argument>
  </arguments>
